{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-tap-highlight-color",
          "scroll-behavior"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 105",
    "not chrome <= 105",
    "not edge <= 105",
    "not firefox <= 104",
    "not safari <= 15.6",
    "not samsung <= 18"
  ]
}